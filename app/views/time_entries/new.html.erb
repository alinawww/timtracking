<div class="row">
  <div class="col-12">
    <h1>Add a project</h1>
    <%= form_for [@my_project, @my_entry] do |f|%>
    <%= f.label :hours%>
    <br>
    <%= f.text_field :hours%>
    <br>
    <%= f.label :minutes%>
    <br>
    <%= f. text_field :minutes%>
    <br>
    <%= f.label :date%>
    <br>
    <%= f. date_field :date%>
    <br>
    <%= f.label :comments%>
    <br>
    <%= f. text_area :comments%>
    <br>
    <%= f.button "Save"%>
    <%end %>
  </div>
</div>
